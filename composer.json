{
	"name": "yocto/yoclib-propertylist",
	"type": "library",
	"description": "This yocLibrary enables your project to encode and decode Property List files in PHP.",
	"keywords":[
		"apple",
		"composer",
		"openstep",
		"php",
		"plist",
		"next",
		"nextstep",
		"yocto",
		"yoclib"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^7||^8",
		"ext-ctype": "*",
		"ext-simplexml": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^7||^8||^9"
	},
	"autoload": {
		"psr-4": {
			"YOCLIB\\PropertyList\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"YOCLIB\\PropertyList\\Tests\\": "tests/"
		}
	}
}